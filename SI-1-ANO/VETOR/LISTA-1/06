}

void  printv ( int vet[], int n){
	int ;
	para (i= 0 ;i<n;i++)
		printf ( "  %d | " ,vet[i]);
	printf ( " \n " );

}

int  num ( int v[], int j, int n){
	int ;
	for (i = 0 ; i < n; i++){
		se (j == v[i])
			retorno eu;	
	}
	retorna  0 ;
}


