int  main (){
	char dna[ 51 ], rna[ 51 ], teste[ 3 ];
	int ;
	
	obtém (dna);
	
	for ( i = 0 ; i < 50 ; i++ ){
		if (dna[i] == ' A ' )
			rna[i] = ' T ' ;
		senão  if (dna[i] == ' T ' )
			rna[i] = ' A ' ;
		senão  if (dna[i] == ' C ' )
			rna[i] = ' G ' ;
		senão  if (dna[i] == ' G ' )
			rna[i] = ' C ' ;
	}
	
	printf ( " \n DNA: %s \n RNA: %s \n " , dna, rna);

}
